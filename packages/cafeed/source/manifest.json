{
  "name": "Notifier for Cafe",
  "version": "1.0.0",
  "description": "Displays your cafe notifications unread count",
  "homepage_url": "https://github.com/cbcruk/webext",
  "manifest_version": 3,
  "icons": {
    "128": "icon.png"
  },
  "permissions": ["alarms", "storage", "notifications", "tabs", "cookies"],
  "host_permissions": ["*://*.naver.com/"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "icon-toolbar-gray.png",
    "default_title": "cafe",
    "default_popup": "popup.html"
  }
}
